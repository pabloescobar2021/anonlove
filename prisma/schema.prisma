generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlite"
}

model User {
  id     String  @id @default(cuid())
  name   String?
  email  String? @unique
  image  String?
  photos Photo[]
}

model Photo {
  id        String   @id @default(cuid())
  url       String   
  userId    String
  user      User     @relation(fields: [userId], references: [id])
  createdAt DateTime @default(now())
}